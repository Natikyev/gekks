def remove_duplicates(arr):
    if not arr:
        return 0
    k = 1  
    for i in range(1, len(arr)):
        if arr[i] != arr[i - 1]:
            arr[k] = arr[i]
            k += 1
    return k, arr[:k]
arr = [1, 2, 2, 3, 4, 4, 4, 5, 5]
length, result = remove_duplicates(arr)
print(result)  
print(length)  
